{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "baseUrl": "src",
    "target": "es6",
    "strict": true,
    "esModuleInterop": true,
    "typeRoots": [
      "node_modules/@types",
      "node_modules/phoenix-typings",
      "@types"
    ],
    "lib": [
      "es2017",
      "esnext"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}
